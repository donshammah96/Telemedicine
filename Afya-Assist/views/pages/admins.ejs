<main>
    <div class="container mt-5">
        <h1 class="mb-4">Admins</h1>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Health Center ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Date Joined</th>
                    <th>Role</th>
                </tr>
            </thead>
            <tbody>
                <% admins.forEach(function(admin) { %>
                <tr>
                    <td><%= admin.admin_id %></td>
                    <td><%= admin.health_center_id %></td>
                    <td><%= admin.first_name %></td>
                    <td><%= admin.last_name %></td>
                    <td><%= admin.email_address %></td>
                    <td><%= admin.phone_number %></td>
                    <td><%= admin.date_joined %></td>
                    <td><%= admin.role %></td>
                </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</main>